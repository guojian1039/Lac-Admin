{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "dianwoung/large-file-upload": "^0.0.3",
        "encore/laravel-admin": "^1.7",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.5",
        "james.xue/login-captcha": "^2.0",
        "laravel-admin-ext/china-distpicker": "^1.4",
        "laravel-admin-ext/ckeditor": "^1.0",
        "laravel-admin-ext/cropper": "^1.0",
        "laravel-admin-ext/grid-lightbox": "^1.0",
        "laravel-admin-ext/media-manager": "^1.1",
        "laravel-admin-ext/redis-manager": "^1.2",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "^3.1",
        "simplesoftwareio/simple-qrcode": "1.3.*"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Common/lib/"
        ],
        "files": [
            "app/Common/function.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
